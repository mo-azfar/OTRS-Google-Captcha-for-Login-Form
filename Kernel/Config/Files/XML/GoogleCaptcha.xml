<?xml version="1.0" encoding="utf-8"?>
<otrs_config version="2.0" init="Config">
    <Setting Name="GoogleCaptcha::CustomerPortalLoginEnabled" Required="0" Valid="1">
        <Description Translatable="1">Enables/disables the functionality of Google Captcha at Customer Portal.</Description>
        <Navigation>GoogleCaptcha::CustomerPortalLoginEnabled</Navigation>
        <Value>
            <Item ValueType="Select" SelectedID="0">
                <Item ValueType="Option" Value="1" Translatable="1">Enabled</Item>
                <Item ValueType="Option" Value="0" Translatable="1">Disabled</Item>
            </Item>
        </Value>
    </Setting>
    <Setting Name="GoogleCaptcha::SiteKey" Required="0" Valid="1">
        <Description Translatable="1">Defines the google recaptcha site key</Description>
        <Navigation>GoogleCaptcha::SiteKey</Navigation>
        <Value>
            <Item ValueType="String" ValueRegex="">SITE_KEY_HERE</Item>
        </Value>
    </Setting>
    <Setting Name="GoogleCaptcha::SecretKey" Required="0" Valid="1">
        <Description Translatable="1">Defines the google recaptcha secret key</Description>
        <Navigation>GoogleCaptcha::SecretKey</Navigation>
        <Value>
            <Item ValueType="String" ValueRegex="">SECRET_KEY_HERE</Item>
        </Value>
    </Setting>
</otrs_config>
